<ul class="list list-group list-group-flush">
<% for (const item of items) { %>
  <li <%= metadataAttrs(item) %> class="list-group-item">
    <a class="listing-link" href="<%- item.path %>">
      <iconify-icon icon="fluent-emoji:link"></iconify-icon>
      <%- item.date %> &#8212; <%= item.title %>
    </a>
  </li>
<% } %>
</ul>
